{
	"cli": {
		"version": ">= 16.15.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal"
		},
		"preview": {
			"distribution": "internal"
		},
		"production": {
			"autoIncrement": true,
			"ios": {
				"resourceClass": "m-medium"
			}
		},
		"development-simulator": {
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true
			},
			"environment": "development"
		}
	},
	"submit": {
		"production": {}
	}
}

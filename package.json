{
  "name": "fortia",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "expo lint",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "node scripts/migrate-database.js migrate",
    "db:status": "node scripts/migrate-database.js status"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@clerk/clerk-expo)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.6.7",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@neondatabase/serverless": "^0.10.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/cli-server-api": "^15.1.3",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "expo": "~53.0.0",
    "expo-apple-authentication": "~7.2.4",
    "expo-auth-session": "^6.2.1",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "^14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-local-authentication": "~16.0.5",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-sensors": "~14.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "nativewind": "^2.0.11",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendar-picker": "^8.0.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-charts": "^1.4.55",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-swiper": "^1.6.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-native-calendar-picker": "^8.0.0",
    "@types/react-test-renderer": "^18.3.0",
    "babel-preset-expo": "~13.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "prettier": "^3.4.2",
    "react-test-renderer": "19.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
